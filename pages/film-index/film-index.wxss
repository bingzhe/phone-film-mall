page {
  padding-bottom: 48px;
}

.swiper-wrap {
  /* margin-top: 20rpx; */
  /* margin: 0 0 10rpx; */
}

.t-swiper {
  height: 240rpx !important;
}

.t-image__swiper {
  width: 100%;
  height: 240rpx !important;
  /* border-radius: 20rpx; */
}

.t-image__search-btn {
  height: 80rpx;
  width: 80rpx;
}
.phone-modal-wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 10rpx 0;
  background: #fff;
  /* height: 50rpx; */
}

.phone-modal-wrapper .modal-wrapper {
  display: flex;
  color: #000;
}

.phone-modal-wrapper .modal-wrapper .tag {
  font-size: 12px;
  background: #23b7eb;
  color: #fff;
  padding: 5rpx 10rpx;
  border-radius: 6rpx;
  margin-right: 10rpx;
}

.phone-modal-wrapper .icon-wrapper {
  display: flex;
}

.phone-modal-wrapper .icon-wrapper text {
  font-size: 14px;
  color: #999;
}

.search-wrapper {
  padding: 10rpx;
  background: #fff;
}

.search-wrapper .t-search__input-container {
  /* border-radius: 32rpx !important; */
  /* height: 64rpx !important; */
  border: 1px solid #23b7eb !important;
  /* border: none !important; */
  border-radius: 8rpx 0 0 8rpx !important;
}

.search-wrapper .t-search__input {
  font-size: 28rpx !important;
  color: rgb(116, 116, 116) !important;
}

.search-button {
  width: 160rpx !important;
  border-radius: 0 8rpx 8rpx 0 !important;
  background-color: #23b7eb !important;
  /* border-color: #ec9a00 !important; */
  border: none !important;
}

.tabs-wrapper {
  padding: 0 15rpx;
  display: flex;
  flex-wrap: wrap;
  background: #f9f9f9;
}

/* .tabs-wrapper .t-tabs {
    background: #f5f5f5;
  } */

.tab-item {
  min-width: 80rpx !important;
  flex: none !important;
  font-size: 14px;
  border-radius: 4px;
  color: #333;
  background-color: #f9f9f9;
  border-color: #f9f9f9;
  box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2),
    0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);
  margin: 10rpx 5rpx;
  padding: 10rpx;
  text-align: center;
  margin-right: 20rpx;
}

.tab-item.tab-item_active {
  font-weight: 400 !important;
  font-size: 14px;
  color: #fff !important;
  background-color: #23b7eb;
  border-color: #23b7eb;
  box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2),
    0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);
}

.tabs-wrapper .t-tabs .t-tabs-nav {
  background-color: transparent;
  line-height: 80rpx;
  font-size: 28rpx;
  color: #333;
}

.tabs-wrapper .t-tabs .t-tabs-scroll {
  border: none !important;
}

/* 半个字 */
.tabs-wrapper .tab.order-nav .order-nav-item.scroll-width {
  min-width: 165rpx;
}

.tabs-wrapper .tab .order-nav-item.active {
  color: #fa550f !important;
}

.tabs-wrapper .tab .bottom-line {
  border-radius: 4rpx;
}

.tabs-wrapper .tab .order-nav-item.active .bottom-line {
  background-color: #fa550f !important;
}

.tabs-wrapper .t-class-item {
  color: #333333 !important;
  font-size: 32rpx;
}

.tabs-wrapper .t-class-track {
  display: none;
  /* background-color: #fa4126 !important;
    height: 6rpx !important;
    border-radius: 4rpx !important;
    width: 48rpx !important; */
}

.t-tabs.t-tabs--top .t-tabs__item,
.t-tabs.t-tabs--bottom .t-tabs__item {
  height: 70rpx !important;
  line-height: 70rpx !important;
}

.t-tabs__item {
  min-width: 80rpx !important;
  flex: none !important;
  font-size: 14px;
  border-radius: 4px;
  color: #000;
  background-color: #e0e0e0;
  border-color: #e0e0e0;
  box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2),
    0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);
  margin: 5rpx 5rpx;
  padding: 0 10rpx;
}

.t-tabs.t-tabs--top .t-tabs__scroll {
  border: none !important;
}

.t-tabs .t-is-active {
  font-weight: 400 !important;
  font-size: 14px;
  color: #fff !important;
  background-color: #1976d2;
  border-color: #1976d2;
  box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2),
    0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);
}

/* .film-list-wrapper {
  } */

.file-item-wrapper {
  border-bottom: 1px solid #ccc;
}

.film-item {
  padding: 30rpx 30rpx 0;
  display: flex;
}

.toggle-expand {
  text-align: center;
}

.film-item:last-child {
  border-bottom: 0px;
}

.film-icon-wrapper {
  display: flex;
  align-items: center;
  width: 180rpx;
  margin-right: 40rpx;
}

.film-icon-wrapper .goods-img {
  width: 130rpx;
  height: 130rpx;
  border-radius: 8rpx;
  margin: 0 10rpx;
}

.film-icon-wrapper .goods-name {
  font-weight: bold;
  word-break: break-all;
  text-align: center;
  font-size: 10px;
  /* border: 1px solid #0052d9; */
  color: #fff;
  background: #61a0e2;
  border-radius: 8rpx;
  padding: 8rpx;
}

.film-content {
  flex: 1;
}

.film-content .film-title {
  font-weight: bold;
  margin: 0 0 20rpx 0;
}

.film-content .phone-modal-list {
  display: flex;
  flex-wrap: wrap;
  height: fit-content;
  overflow: hidden;
}

.file-contnet .phone-modal-list.phone-modal-list_expand {
  height: 0;
}

.film-content .firm-selects {
  display: flex;
  flex-wrap: wrap;
}

.firm-selects .firm-select {
  background: #23b7eb;
  color: #fff;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  border-radius: 8rpx;
  font-size: 18px;
  padding: 5rpx 10rpx;
}

.film-content .phone-modal-list .modal {
  font-size: 12px;
  padding: 5rpx 10rpx;
  background: #f1eee9;
  margin-right: 16rpx;
  margin-bottom: 16rpx;
  border-radius: 8rpx;
  box-shadow: 0px 3px 1px -2px rgba(0, 0, 0, 0.2),
    0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 1px 5px 0px rgba(0, 0, 0, 0.12);
}

.film-content .phone-modal-list .modal.select {
  background: #23b7eb;
  color: #fff;
}

.film-price {
  text-align: right;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  color: #fa4126;
  font-size: 14px;
}

.film-list-empty {
  text-align: center;
}

.film-list-empty .empty-class {
  width: 200rpx;
  height: 200rpx;
}

.film-list-empty .empty-tip {
  color: #999;
}

.search-cus-button {
  background: #61a0e2 !important;
  /* border-color: #0594fa !important; */
  border: none !important;
}

.search-list-wrapper {
  margin: 0 30rpx;
}

.search-list-wrapper .search-name-item {
  font-size: 14px;
  line-height: 60rpx;
  border-bottom: 1px solid #ccc;
}

.search-list-wrapper .is-color {
  color: red;
}

.notice {
  display: flex;
  align-content: center;
  padding: 16rpx 25rpx;
  background: #f5f5f5;
  align-items: center;
  margin-bottom: 10rpx;
}

.sound-icon-wrapper {
  display: flex;
}

.sound-img {
  height: 40rpx;
  width: 78rpx;
  margin-right: 15rpx;
}

.voice {
  margin-right: 5rpx;
  margin-top: 2rpx;
  color: #fa6016;
}

.left-box {
  position: relative;
  display: flex;
  align-items: center;
  flex: 1;
  width: 400rpx;
}

.left-text {
  position: absolute;
  left: 0;
  width: 40rpx;
  height: 100%;
  background: linear-gradient(
    to left,
    rgba(241, 241, 241, 0),
    rgba(241, 241, 241, 1)
  );
  z-index: 99;
}

.right-text {
  position: absolute;
  right: -1rpx;
  width: 40rpx;
  height: 100%;
  background: linear-gradient(
    to left,
    rgba(241, 241, 241, 1),
    rgba(241, 241, 241, 0)
  );
  z-index: 99;
}

.content-box {
  overflow: hidden;
}

.content-text {
  color: #5e5e5e;
  white-space: nowrap;
  font-size: 34rpx;
}

.popup-content {
  height: 650rpx;
  width: 650rpx;
  /* display: flex; */
  /* justify-content: center; */
  align-items: center;
  box-sizing: border-box;
  padding: 80rpx 0 0 40rpx;
  position: relative;
}

.popup-title {
  font-size: 52rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 10rpx;
}

.popup-content image {
  height: 650rpx;
  width: 650rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
